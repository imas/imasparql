    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">illuminationSTARS</rdfs:label>
    <imas:nameKana xml:lang="ja">いるみねーしょんすたーず</imas:nameKana>
    <schema:member rdf:resource="Sakuragi_Mano"/>
    <schema:member rdf:resource="Hachimiya_Meguru"/>
    <schema:member rdf:resource="Kazano_Hiori"/>
    <imas:Color rdf:datatype="http://www.w3.org/2001/XMLSchema#hexBinary">FFA200</imas:Color>
    <schema:description xml:lang="ja">「瞳に輝く、無限の可能性」小さな光を宿す少女たちで結成された、新世代のアイドルユニット。今はまだかすかな輝きでも、時にぶつかり時に支え合いながら、一等星を目指して歩んでいく!</schema:description>
    <rdf:type rdf:resource="https://sparql.crssnky.xyz/imasrdf/URIs/imas-schema.ttl#Unit"/>
  </rdf:Description>
  <rdf:Description rdf:about="L%27Antica">
